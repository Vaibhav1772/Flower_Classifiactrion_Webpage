{
    "version": 2,
    "builds": [
      {
        "src": "Backend/FASTAPI/Backend.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.10" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Backend/FASTAPI/Backend.py"
      }
    ],
    "buildCommand": "pip install -r requirements.txt"
  }
  